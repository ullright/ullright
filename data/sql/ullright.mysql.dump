INSERT INTO `ull_wiki` VALUES (235,1,0,'en','<p>This is the first entry since</p>\r\n<ul>\r\n    <li>there is no other entry yet</li>\r\n    <li>all other test entries have been deleted ;-)</li>\r\n</ul>','My first entry','',NULL,1,NULL,NULL,1,'2008-04-01 10:43:03',1,'2008-04-02 10:49:31'),(236,2,0,'de','<h1>Hallo 123</h1>\r\n<p>Test:</p>\r\n<ul>\r\n    <li>blabla</li>\r\n    <li>dieses:</li>\r\n    <li>jenes</li>\r\n    <li>quack</li>\r\n</ul>','test','',NULL,1,NULL,NULL,1,'2008-04-02 08:59:58',1,'2008-04-02 10:43:23'),(237,2,0,'de','<h1>Hallo 123</h1>\r\n<p>Test:</p>\r\n<ul>\r\n    <li>blabla</li>\r\n    <li><em>dies</em>es:</li>\r\n    <li>jenes</li>\r\n    <li>quack</li>\r\n</ul>','test','',NULL,2,NULL,NULL,1,'2008-04-02 08:59:58',1,'2008-04-02 10:43:23'),(238,2,0,'de','<h1>Hallo 123</h1>\r\n<p>Test:</p>\r\n<ul>\r\n    <li>bl<strong>abla</strong></li>\r\n    <li><em>dies</em>es:</li>\r\n    <li>jenes</li>\r\n    <li>quack</li>\r\n</ul>','test','',NULL,3,NULL,NULL,1,'2008-04-02 08:59:58',1,'2008-04-02 10:43:23'),(239,2,0,'de','<h1>Hallo 123</h1>\r\n<p>Test:</p>\r\n<ul>\r\n    <li>bl<strong>abla</strong></li>\r\n    <li><em>dies</em>es:</li>\r\n    <li>jenes</li>\r\n    <li>quack</li>\r\n</ul>\r\n<p><a href=\"/uploads/assets/file/indent1.png\">schnu</a></p>','test','',NULL,4,NULL,NULL,1,'2008-04-02 08:59:58',1,'2008-04-02 10:44:58'),(240,1,1,'de','<h1>Test</h1>\r\n<p>&nbsp;</p>\r\n<p>This is the first entry since</p>\r\n<ul>\r\n    <li>there is no other entry yet</li>\r\n    <li>all other test entries have been deleted ;-)</li>\r\n</ul>\r\n<p>&nbsp;</p>\r\n<p>Testimage: <img width=\"48\" height=\"49\" src=\"/uploads/assets/image/test_image.png\" alt=\"test_image\" /></p>\r\n<p>&nbsp;</p>\r\n<p><a href=\"/uploads/assets/test_doc.pdf\">Testlink to document</a></p>','My first entry','',NULL,2,NULL,NULL,1,'2008-04-01 10:43:03',1,'2008-04-02 10:49:31'),(241,3,1,'en','<h2>Problem:</h2>\r\n<ul>\r\n    <li>All lights are blinking</li>\r\n</ul>\r\n<h2>Solution:</h2>\r\n<ul>\r\n    <li>Press [CTRL]-[RESET]</li>\r\n    <li>Enter service-code &quot;fix ojda&quot;</li>\r\n    <li>Reboot</li>\r\n</ul>\r\n<h2>&nbsp;</h2>','how to fix team-printer031','',NULL,1,NULL,NULL,1,'2008-06-09 12:07:53',1,'2008-06-09 12:07:53');
